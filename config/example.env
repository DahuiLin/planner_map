# Example configuration file for Planner Map

# Web Interface Configuration
WEB_HOST=0.0.0.0
WEB_PORT=8000
WEB_RELOAD=false

# ROS2 Configuration
ROS_DOMAIN_ID=0
# RMW Implementation: Use rmw_cyclonedds_cpp or rmw_fastrtps_cpp (default in ROS2 Humble)
RMW_IMPLEMENTATION=rmw_cyclonedds_cpp

# Map Configuration
MAP_RESOLUTION=0.05
MAP_WIDTH=100
MAP_HEIGHT=100
MAP_ORIGIN_X=-2.5
MAP_ORIGIN_Y=-2.5

# Planner Configuration
PLANNING_ALGORITHM=simple
UPDATE_RATE=10.0

# Logging
LOG_LEVEL=INFO

# Note: Copy this file to .env and modify as needed
# Docker Compose will automatically load .env file
